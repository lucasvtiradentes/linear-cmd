# E2E Test Configuration
# Copy this file to .env.test and fill with your test credentials

# Linear API Keys for E2E tests
LINEAR_TEST_API_KEY_WORK=your-work-api-key-here
LINEAR_TEST_API_KEY_PERSONAL=your-personal-api-key-here

# Test Issue IDs (create these in your Linear workspace)
LINEAR_TEST_ISSUE_WORK=WAY-123
LINEAR_TEST_ISSUE_PERSONAL=PERSONAL-123

# Test Issue URLs
LINEAR_TEST_ISSUE_URL_WORK=https://linear.app/yourworkspace/issue/WORK-123/test-issue
LINEAR_TEST_ISSUE_URL_PERSONAL=https://linear.app/personalworkspace/issue/PERSONAL-123/test-issue

# Skip E2E tests if not configured
SKIP_E2E_TESTS=false
